{
  "name": "@abc-map/shared",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'",
    "clean-build": "yarn run clean && yarn run build",
    "lint": "eslint './src/**/*.ts' --max-warnings=0",
    "lint-fix": "yarn run lint --fix",
    "test": "tsc && CI=true jest src --verbose",
    "test:interactive": "jest src --watch",
    "coverage": "tsc && CI=true jest src --coverage",
    "dep-check": "depcruise -c .dependency-cruiser.js src"
  },
  "dependencies": {
    "jszip": "^3.7.1",
    "loglevel": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^26.0.24",
    "@types/jszip": "^3.4.1",
    "@types/node": "^14.17.11",
    "@types/semver": "^7.3.8",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^26.6.3",
    "dependency-cruiser": "^10.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "semver": "^7.3.5",
    "sinon": "^11.1.2",
    "typescript": "^4.3.5"
  }
}
