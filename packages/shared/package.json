{
  "name": "@abc-map/shared",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "clean-build": "yarn run clean && yarn run build",
    "coverage": "tsc && CI=true jest src --coverage",
    "dep-check": "depcruise -c .dependency-cruiser.js src",
    "lint": "eslint './src/**/*.ts' --max-warnings=0",
    "lint-fix": "yarn run lint --fix",
    "test": "tsc && CI=true jest src --verbose",
    "test:interactive": "jest src --watch",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'"
  },
  "dependencies": {
    "jszip": "^3.7.1",
    "loglevel": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.0.3",
    "@types/jszip": "^3.4.1",
    "@types/node": "^14.17.11",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-jest": "^27.4.5",
    "blob-polyfill": "^6.0.20211015",
    "dependency-cruiser": "^11.2.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "semver": "^7.3.5",
    "sinon": "^12.0.1",
    "typescript": "^4.5.4"
  }
}
