<WMS_Capabilities
  version="1.3.0"
  xmlns="http://www.opengis.net/wms"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/wms http://schemas.opengis.net/wms/1.3.0/capabilities_1_3_0.xsd">
  <Service>
    <Name>WMS</Name>
    <Title>Fake WMS Raster service</Title>
    <Abstract>This service allows E2E tests</Abstract>
    <KeywordList>
      <Keyword>WMS</Keyword>
      <Keyword>Map</Keyword>
      <Keyword>Fake</Keyword>
    </KeywordList>
    <OnlineResource xlink:href="http://localhost:3010" xmlns:xlink="http://www.w3.org/1999/xlink"/>
    <ContactInformation>
      <ContactPersonPrimary>
        <ContactPerson>Nobody</ContactPerson>
        <ContactOrganization>Nobody Incorporation</ContactOrganization>
      </ContactPersonPrimary>
      <ContactPosition>custodian</ContactPosition>
      <ContactAddress>
        <AddressType/>
        <Address>No Where</Address>
        <City>No City</City>
        <StateOrProvince/>
        <PostCode>12345</PostCode>
        <Country>France</Country>
      </ContactAddress>
      <ContactVoiceTelephone/>
      <ContactFacsimileTelephone/>
      <ContactElectronicMailAddress>nowhere@nobody.fr</ContactElectronicMailAddress>
    </ContactInformation>
    <Fees>licences</Fees>
    <AccessConstraints>
      Send me beers pals !
    </AccessConstraints>
    <LayerLimit>2</LayerLimit>
    <MaxWidth>10000</MaxWidth>
    <MaxHeight>10000</MaxHeight>
  </Service>
  <Capability>
    <Request>
      <GetCapabilities>
        <Format>text/xml</Format>
        <DCPType>
          <HTTP>
            <Get>
              <OnlineResource xlink:href="http://localhost:3010?" xlink:type="simple" xmlns:xlink="http://www.w3.org/1999/xlink"/>
            </Get>
          </HTTP>
        </DCPType>
      </GetCapabilities>
      <GetMap>
        <Format>image/jpeg</Format>
        <Format>image/png</Format>
        <Format>image/tiff</Format>
        <Format>image/geotiff</Format>
        <Format>image/x-bil;bits=32</Format>
        <Format>text/asc</Format>
        <DCPType>
          <HTTP>
            <Get>
              <OnlineResource xlink:href="http://localhost:3010?" xlink:type="simple" xmlns:xlink="http://www.w3.org/1999/xlink"/>
            </Get>
          </HTTP>
        </DCPType>
      </GetMap>
      <GetFeatureInfo>
        <Format>text/plain</Format>
        <Format>text/html</Format>
        <Format>text/xml</Format>
        <Format>application/xml</Format>
        <Format>application/json</Format>
        <DCPType>
          <HTTP>
            <Get>
              <OnlineResource xlink:href="http://localhost:3010?" xlink:type="simple" xmlns:xlink="http://www.w3.org/1999/xlink"/>
            </Get>
          </HTTP>
        </DCPType>
      </GetFeatureInfo>
    </Request>
    <Exception>
      <Format>XML</Format>
    </Exception>
    <Layer>
      <Layer queryable="1">
        <Name>LAYER1</Name>
        <Title>The first layer</Title>
        <Abstract>A very good layer, the first one</Abstract>
        <KeywordList>
          <Keyword>First</Keyword>
          <Keyword>Layer</Keyword>
        </KeywordList>
        <CRS>IGNF:RGR92GEO</CRS>
        <CRS>IGNF:RGM04GEO</CRS>
        <CRS>IGNF:CSG67UTM21</CRS>
        <CRS>EPSG:3312</CRS>
        <CRS>EPSG:3313</CRS>
        <EX_GeographicBoundingBox>
          <westBoundLongitude>-63.372521503</westBoundLongitude>
          <eastBoundLongitude>55.925864577</eastBoundLongitude>
          <southBoundLatitude>-21.475586668</southBoundLatitude>
          <northBoundLatitude>51.312119281</northBoundLatitude>
        </EX_GeographicBoundingBox>
        <BoundingBox CRS="EPSG:3857" maxx="6225638.766865120" maxy="6676690.825125160" minx="-7054596.823974940" miny="-2448678.407635810"/>
        <MinScaleDenominator>267.857142857</MinScaleDenominator>
        <MaxScaleDenominator>559082264.285714269</MaxScaleDenominator>
      </Layer>
      <Layer queryable="1">
        <Name>LAYER2</Name>
        <Title>The second layer</Title>
        <Abstract>A good layer too, the second</Abstract>
        <KeywordList>
          <Keyword>Second</Keyword>
          <Keyword>Layer</Keyword>
        </KeywordList>
        <CRS>IGNF:RGR92GEO</CRS>
        <CRS>IGNF:RGM04GEO</CRS>
        <CRS>IGNF:CSG67UTM21</CRS>
        <CRS>EPSG:3312</CRS>
        <CRS>EPSG:3313</CRS>
        <EX_GeographicBoundingBox>
          <westBoundLongitude>-63.372521503</westBoundLongitude>
          <eastBoundLongitude>55.925864577</eastBoundLongitude>
          <southBoundLatitude>-21.475586668</southBoundLatitude>
          <northBoundLatitude>51.312119281</northBoundLatitude>
        </EX_GeographicBoundingBox>
        <BoundingBox CRS="EPSG:3857" maxx="6225638.766865120" maxy="6676690.825125160" minx="-7054596.823974940" miny="-2448678.407635810"/>
        <MinScaleDenominator>267.857142857</MinScaleDenominator>
        <MaxScaleDenominator>559082264.285714269</MaxScaleDenominator>
      </Layer>
    </Layer>
  </Capability>
</WMS_Capabilities>
