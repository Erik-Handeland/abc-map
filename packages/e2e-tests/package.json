{
  "name": "@abc-map/e2e-tests",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "e2e-test:before-merge": "cypress run --config-file ./config/ci.json --spec 'src/integration/ci/**/*'",
    "e2e-test:interactive": "cypress open --config-file ./config/development.json",
    "clean": "rm -rf build",
    "build": "tsc",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'",
    "clean-build": "yarn run clean && yarn run build",
    "lint": "eslint './src/**/*.ts' --fix --max-warnings=0",
    "dep-check": "depcruise -c .dependency-cruiser.js src"
  },
  "dependencies": {
    "@abc-map/shared-entities": "0.0.1",
    "@abc-map/frontend-shared": "0.0.1",
    "@types/cypress": "^1.1.3",
    "cypress-file-upload": "^4.1.1",
    "uuid-random": "^1.3.2",
    "cypress": "^6.2.0",
    "typescript": "4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "dependency-cruiser": "9.19.1",
    "eslint-plugin-cypress": "^2.11.2"
  },
  "files": [
    "build"
  ]
}
