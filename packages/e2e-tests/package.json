{
  "name": "@abc-map/e2e-tests",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "e2e-test": "cypress run",
    "e2e-test:ci": "cypress run --spec src/integration/ci/**/*",
    "e2e-test:interactive": "cypress open",
    "clean": "rm -rf build",
    "build": "tsc",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'",
    "clean-build": "yarn run clean && yarn run build",
    "lint": "eslint './src/**/*.ts' --fix --max-warnings=0",
    "dep-check": "depcruise -c .dependency-cruiser.js src"
  },
  "dependencies": {
    "@abc-map/shared-entities": "0.0.1",
    "cypress-file-upload": "^4.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@types/cypress": "^1.1.3",
    "cypress": "^6.2.0",
    "typescript": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "dependency-cruiser": "9.19.1"
  }
}
