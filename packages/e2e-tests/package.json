{
  "name": "@abc-map/e2e-tests",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "files": [
    "build"
  ],
  "scripts": {
    "e2e:ci": "cypress run --browser chromium --headless --config-file ./config/ci.json --spec 'src/integration/**/*'",
    "e2e:dev": "cypress run --browser chromium --headless --config-file ./config/development.json --spec 'src/integration/**/*'",
    "e2e:dev:interactive": "cypress open --config-file ./config/development.json",
    "clean": "rm -rf build",
    "build": "tsc",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'",
    "clean-build": "yarn run clean && yarn run build",
    "lint": "eslint 'src/**/*.ts' --max-warnings=0",
    "lint-fix": "yarn run lint --fix",
    "dep-check": "depcruise -c .dependency-cruiser.js src"
  },
  "dependencies": {
    "@abc-map/shared": "0.1.0",
    "@types/cypress": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cypress": "^7.7.0",
    "cypress-file-upload": "^5.0.7",
    "dependency-cruiser": "10.0.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "uuid-random": "^1.3.2"
  }
}
