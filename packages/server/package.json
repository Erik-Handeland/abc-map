{
  "name": "@abc-map/server",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "main": "build/main.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "clean-build": "yarn run clean && yarn run build",
    "coverage": "nyc mocha --trace-warnings 'build/**/*.test.js'",
    "dep-check": "depcruise -c .dependency-cruiser.js src",
    "lint": "eslint './src/**/*.ts' --max-warnings=0",
    "lint-fix": "yarn run lint --fix",
    "start:e2e": "node ./build/main.js",
    "start:watch": "nodemon --trace-warnings --watch build --watch resources ./build/main.js",
    "test": "mocha --trace-warnings 'build/**/*.test.js'",
    "test:interactive": "mocha --watch --trace-warnings 'build/**/*.test.js'",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'"
  },
  "dependencies": {
    "@abc-map/shared": ">1.0.0-rc",
    "accept-language-parser": "^1.5.0",
    "ajv": "8.8.2",
    "bson": "^4.6.0",
    "fastify": "^3.25.2",
    "fastify-error": "^0.3.1",
    "fastify-jwt": "^4.1.3",
    "fastify-metrics": "^8.0.0",
    "fastify-multipart": "^5.2.1",
    "fastify-rate-limit": "^5.7.0",
    "fastify-sensible": "^3.1.2",
    "fastify-static": "^4.5.0",
    "glob": "^7.2.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^2.2.0",
    "mongodb": "^4.2.2",
    "nodemailer": "^6.7.2",
    "point-of-view": "^5.0.0",
    "prom-client": "^14.0.1",
    "source-map-support": "^0.5.21",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@types/accept-language-parser": "^1.5.3",
    "@types/chai": "^4.3.0",
    "@types/geojson": "^7946.0.8",
    "@types/glob": "^7.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.8",
    "@types/mocha": "^9.1.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^14.17.11",
    "@types/nodemailer": "^6.4.4",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chai": "^4.3.4",
    "dependency-cruiser": "^11.2.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "form-data": "^4.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "sinon": "^12.0.1",
    "typescript": "^4.5.4"
  },
  "docker": {
    "imageName": "server"
  }
}
