{
  "name": "@abc-map/abc-cli",
  "version": "0.0.1",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'",
    "clean-build": "yarn run clean && yarn run build",
    "lint": "eslint './src/**/*.ts' --fix --max-warnings=0",
    "coverage": "nyc mocha --trace-warnings 'build/**/*.test.js'",
    "test": "mocha --trace-warnings 'build/**/*.test.js'",
    "dep-check": "depcruise -c .dependency-cruiser.js src"
  },
  "dependencies": {
    "lerna": "^4.0.0",
    "loglevel": "^1.7.1",
    "wait-on": "^5.2.1",
    "source-map-support": "^0.5.19",
    "verdaccio": "^5.0.1",
    "nanoid": "^3.1.22"
  },
  "devDependencies": {
    "@types/wait-on": "^5.2.0",
    "@types/node": "^14.14.16",
    "@types/mocha": "^8.0.4",
    "@types/chai": "^4.2.14",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.1",
    "typescript": "4.1.5",
    "nyc": "^15.1.0",
    "mocha": "^8.2.1",
    "chai": "^4.2.0",
    "dependency-cruiser": "9.19.1"
  },
  "files": [
    "build"
  ]
}
