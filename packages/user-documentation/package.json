{
  "name": "@abc-map/user-documentation",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "build": "webpack",
    "watch": "webpack -w",
    "clean-build": "yarn run clean && yarn run build",
    "dep-check": "depcruise -c .dependency-cruiser.js src",
    "lint": "eslint './src/**/*.ts' --max-warnings=0 && prettier --check src/**/*.md",
    "lint-fix": "eslint './src/**/*.ts' --fix --max-warnings=0 && prettier --write src/**/*.md"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "dependency-cruiser": "^10.0.6",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-loader": "^2.1.2",
    "markdown-loader": "^6.0.0",
    "prettier": "^2.3.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  }
}
