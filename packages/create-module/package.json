{
  "name": "@abc-map/create-module",
  "version": "1.0.10",
  "license": "AGPL-3.0-or-later",
  "main": "src/main.js",
  "bin": {
    "create-module": "build/main.js"
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && chmod +x build/main.js",
    "clean": "rm -rf build",
    "clean-build": "yarn run clean && yarn run build",
    "dep-check": "depcruise -c .dependency-cruiser.js src",
    "dev": "node scripts/integration-test.js",
    "lint": "eslint './src/**/{*.tsx,*.ts}' --max-warnings=0 && sort-package-json --check",
    "lint-fix": "eslint './src/**/{*.tsx,*.ts}' --max-warnings=0 --fix && sort-package-json",
    "public-publish": "scripts/public-publish.sh",
    "test": "mocha --trace-warnings 'build/**/*.test.js'",
    "test:interactive": "mocha --watch --trace-warnings 'build/**/*.test.js'",
    "watch": "tsc --watch | sed -u 's/\\x1Bc//g'"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "axios": "^0.26.1",
    "chalk": "^4.1.2",
    "command-line-args": "^5.2.1",
    "hasbin": "^1.2.3",
    "loglevel": "^1.8.0",
    "sort-package-json": "^1.55.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/chai": "^4.3.1",
    "@types/command-line-args": "^5.2.0",
    "@types/hasbin": "^1.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.13",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "dependency-cruiser": "^11.2.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nanoid": "^3.3.3",
    "prettier": "^2.5.1",
    "sinon": "^13.0.2",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "volta": {
    "node": "16.15.0"
  }
}
