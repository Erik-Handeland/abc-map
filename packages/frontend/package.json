  "license": "GPL-3.0-or-later",
  "proxy": "http://127.0.0.1:10082",
  "scripts": {
    "start:e2e": "node scripts/e2e-server.js",
    "start:watch": "react-scripts start",
    "lint": "eslint './src/**/{*.tsx,*.ts}' --fix --max-warnings=0",
    "build": "CI=true react-scripts build && yarn run build:check",
    "build:check": "./scripts/check-build.js",
    "clean": "rm -rf build",
    "clean-build": "yarn run clean && yarn run build",
    "test": "yarn run typecheck && CI=true react-scripts test --coverage",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --noEmit --watch | sed -u 's/\\x1Bc//g'",
    "test:interactive": "react-scripts test",
    "eject": "react-scripts eject",
    "dep-check": "depcruise -c .dependency-cruiser.js src"
  },
