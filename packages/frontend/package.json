    "test": "CI=true react-app-rewired test --verbose",
    "test:interactive": "react-app-rewired test",
    "type-check": "tsc --noEmit",
    "watch": "tsc --noEmit --watch | sed -u 's/\\x1Bc//g'",
    "dep-check": "depcruise -c .dependency-cruiser.js src",
    "bundle-analyze": "node scripts/bundle-analyze.js",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
